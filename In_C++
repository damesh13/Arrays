//we are going to see only one dimensional arrays 
#include <iostream>
using namespace std ;
int main() {
    //Here i created an array and I also given the values 
    int a[5]={1,2,3,4,5};
    
    // the first index always start with 0 and last index always end with size-1
    //printing first element and last element in the array 
    cout << a[0]<<" ";
    cout << a[4]<<" ";
    
    //inserting element into the array 
    a[0]=10;
    cout <<a[0]<<" ";
    
    //printing all the elements in the array
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
    
    

    return 0;
}
